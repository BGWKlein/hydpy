<?xml version="1.0" encoding="UTF-8"?>
<config
        xmlns="https://github.com/tyralla/hydpy/tree/master/hydpy/conf/HydPy2FEWS.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://github.com/tyralla/hydpy/tree/master/hydpy/conf/HydPy2FEWS.xsd HydPy2FEWS.xsd">

    <timegrid>
        <firstdate>1996-01-01T00:00:00</firstdate>
        <lastdate>1996-01-06T00:00:00</lastdate>
        <stepsize>1d</stepsize>
    </timegrid>

    <selections>
        headwaters
        streams
    </selections>

    <devices/>

    <sequences>

        <filetype>
            <general>npy</general>
        </filetype>

        <inputs>
            <input info="all input data">
                <selections>
                    headwaters
                    nonheadwaters
                </selections>
                <filetype>
                    <inputs>nc</inputs>
                </filetype>
                <series>
                    <hland_v1.inputs.p/>
                    <hland_v1.inputs.t/>
                    <hland_v1.inputs.tn/>
                    <hland_v1.inputs.epn/>
                </series>
            </input>
        </inputs>

        <outputs>

            <output info="precipitation">
                <selections>
                    headwaters
                </selections>
                <filetype>
                    <inputs>asc</inputs>
                    <fluxes>npy</fluxes>
                </filetype>
                <series>
                    <hland_v1.inputs.p/>
                    <hland_v1.fluxes.pc/>
                    <hland_v1.fluxes.tf/>
                </series>
            </output>

            <output info="soilmoisture">
                <selections>
                    complete
                </selections>
                <filetype>
                    <general>nc</general>
                </filetype>
                <selections>
                    ToDo
                </selections>
                <devices>
                    ToDo
                </devices>
                <series>
                    <hland_v1.states.sm/>
                </series>
            </output>

            <output info="averaged">
                <series>
                    <nodes.sim/>
                    <nodes.obs/>
                    <hland_v1.fluxes.pc/>
                    <hland_v1.fluxes.tf/>
                    <hland_v1.states.sm/>
                </series>
            </output>

        </outputs>

    </sequences>

</config>
