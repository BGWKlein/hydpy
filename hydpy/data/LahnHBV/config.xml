<?xml version="1.0" encoding="UTF-8"?>
<config
        xmlns="https://github.com/tyralla/hydpy/tree/master/hydpy/conf/HydPy2FEWS.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://github.com/tyralla/hydpy/tree/master/hydpy/conf/HydPy2FEWS.xsd HydPy2FEWS.xsd">

    <timegrid>
        <firstdate>1996-01-01T00:00:00</firstdate>
        <lastdate>1996-01-06T00:00:00</lastdate>
        <stepsize>1d</stepsize>
    </timegrid>

    <filetype>
        <general>npy</general>
    </filetype>

    <outputs>
        <output info="precipitation">
            <filetype>
                <inputs>asc</inputs>
                <fluxes>npy</fluxes>
            </filetype>
            <sequences>
                <hland_v1.inputs.p/>
                <hland_v1.fluxes.pc/>
                <hland_v1.fluxes.tf/>
            </sequences>
        </output>
        <output info="soilmoisture">
            <filetype>
                <general>nc</general>
            </filetype>
            <selections>
                ToDo
            </selections>
            <devices>
                ToDo
            </devices>
            <sequences>
                <hland_v1.states.sm/>
            </sequences>
        </output>
        <output info="averaged">
            <sequences>
                <nodes.sim/>
                <nodes.obs/>
                <hland_v1.fluxes.pc/>
                <hland_v1.fluxes.tf/>
                <hland_v1.states.sm/>
            </sequences>
        </output>
    </outputs>


</config>
